@using DIplomaProject.Frontend.Models.Dto.ShopUser
@model ShopUserPostDto

<h2>Create ShopUser</h2>

<form asp-action="Create">
    <div class="form-group">
        <label asp-for="ShopId" class="control-label">ShopId</label>
        <input asp-for="ShopId" class="form-control" required />
    </div>
    <div class="form-group">
        <label asp-for="UserId" class="control-label">UserId</label>
        <input asp-for="UserId" class="form-control" required />
    </div>
    <div class="form-group">
        <input type="submit" value="Create" class="btn btn-primary" />
    </div>
</form>
